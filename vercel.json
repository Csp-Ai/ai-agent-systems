{
  "version": 2,
  "cleanUrls": true,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    { "handle": "filesystem" },
    { "source": "/(.*)", "destination": "/frontend/index.html" }
  ]
}
